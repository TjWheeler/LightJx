{
    "compilerOptions": {
      "target": "ES2016",
      "module": "ES6",
      "esModuleInterop": true,
      "strict": true,
      "rootDir": "src",
      "outDir": "dist",
      "sourceMap": true,
      "declaration": true,
      "moduleResolution": "node",
      "removeComments": true,
      "preserveConstEnums": true,
      "noImplicitAny": true,
    },
    "include": ["src", "../__tests__"],
  }
